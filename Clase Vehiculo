public class Vehiculo {
    private String placa;
    private int capacidadMaximaGalones;
    private int galonesActuales;

    public Vehiculo(String placa, int capacidadMaximaGalones, int galonesActuales) {
        this.placa = placa;
        this.capacidadMaximaGalones = capacidadMaximaGalones;
        this.galonesActuales = galonesActuales;
    }

    public String getPlaca() {
        return placa;
    }

    public int getCapacidadMaximaGalones() {
        return capacidadMaximaGalones;
    }

    public int getGalonesActuales() {
        return galonesActuales;
    }

    public void setGalonesActuales(int galonesActuales) {
        this.galonesActuales = galonesActuales;
    }
}
